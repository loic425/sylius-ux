{
    "name": "loic425/sylius-ux",
    "description": "Sylius UX package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/property-access": "^5.4 || ^6.0",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "doctrine/collections": "^1.6",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpspec/phpspec": "^7.2",
        "phpunit/phpunit": "^9.5",
        "symfony/debug-bundle": "^6.1",
        "symfony/dotenv": "^6.1",
        "symfony/framework-bundle": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sylius\\Ux\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Sylius\\Ux\\": "spec/",
            "Tests\\Sylius\\Ux\\": "tests/",
            "Tests\\Sylius\\Ux\\Application\\": "tests/Application/src"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    }
}
