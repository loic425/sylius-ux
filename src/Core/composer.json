{
    "name": "loic425/sylius-ux-core",
    "description": "Sylius UX Core package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "knplabs/knp-menu-bundle": "^3.2",
        "symfony/property-access": "^5.4 || ^6.0",
        "twig/twig": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Sylius\\Ux\\Core": "./src"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    }
}
